	<DiagramRectangle xsi:type="DocRectangle" X="306.9668" Y="3276.4666" Width="544.73333" Height="100" />
	<Attributes>
		<DocAttribute Name="InnerCurves" UniqueId="f5a968fa-e9ba-423c-9752-52657d57b5bc" DefinedType="IfcCurve" AttributeFlags="0" AggregationType="3" AggregationFlag="0" AggregationLower="1" AggregationUpper="0">
